{
    "codeShareLink.apiUrl": "http://localhost:3000",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "eslint.validate": [
        "javascript"
    ],
    "files.exclude": {
        "out": false,
        "node_modules": true
    },
    "search.exclude": {
        "out": true,
        "node_modules": true
    }
}
